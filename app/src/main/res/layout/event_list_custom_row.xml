<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent" android:layout_height="wrap_content"
    android:background="@android:color/background_light"
    android:id="@+id/relativeLayoutEventListCustomRow"
    android:gravity="center_horizontal"
    android:elevation="@dimen/abc_alert_dialog_button_bar_height"
    android:paddingBottom="0dp">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:text="New Text"
        android:id="@+id/textViewVTAEventListTitle"
        android:layout_alignParentTop="false"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:textColor="#f5f6f9"
        android:textSize="18sp"
        android:gravity="center_vertical|center_horizontal"
        android:textStyle="bold"
        android:layout_margin="15dp"
        android:background="#7d7ef5"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="30dp"
        android:layout_marginRight="30dp"
        android:autoText="false" />

    <TextView
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Place"
        android:id="@+id/textView6"
        android:layout_below="@+id/textViewVTAEventListTitle"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/colorPrimary"
        android:textSize="15sp"
        android:paddingLeft="20dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewVTAEventListPlace"
        android:layout_below="@+id/textViewVTAEventListTitle"
        android:layout_toRightOf="@+id/textView6"
        android:layout_toEndOf="@+id/textView6"
        android:textColor="@color/colorPrimary"
        android:textSize="15sp" />

    <TextView
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Duration"
        android:id="@+id/textView2"
        android:layout_below="@+id/textViewVTAEventListPlace"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/colorPrimary"
        android:textSize="15sp"
        android:paddingLeft="20dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewVTAEventListDuration"
        android:layout_below="@+id/textViewVTAEventListPlace"
        android:layout_toRightOf="@+id/textView2"
        android:layout_toEndOf="@+id/textView2"
        android:textColor="@color/colorPrimary"
        android:textSize="15sp" />

    <TextView
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Reservations"
        android:id="@+id/textView7"
        android:layout_below="@+id/textViewVTAEventListDuration"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/colorPrimary"
        android:textSize="15sp"
        android:paddingLeft="20dp"
        android:layout_marginBottom="5dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewVTAEventListReservations"
        android:layout_below="@+id/textViewVTAEventListDuration"
        android:layout_toRightOf="@+id/textView7"
        android:layout_toEndOf="@+id/textView7"
        android:textColor="@color/colorPrimary"
        android:textSize="15sp"
        android:layout_marginBottom="5dp" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="40dp"
        android:id="@+id/imageViewHotel"
        android:src="@drawable/hotel_icon"
        android:layout_below="@+id/textView7"
        android:layout_alignLeft="@+id/textViewVTAEventListTitle"
        android:layout_alignStart="@+id/textViewVTAEventListTitle"
        android:layout_marginLeft="20dp" />



    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Hotel Res"
        android:id="@+id/textViewEventListHotelResDetails"
        android:layout_below="@+id/textViewVTAEventListReservations"

        android:layout_alignBaseline="@+id/imageViewHotel"
        android:layout_toRightOf="@+id/imageViewHotel"
        android:layout_toEndOf="@+id/imageViewHotel"
        android:gravity="bottom"
        android:layout_marginLeft="10dp"
        android:textColor="#181da0"
        android:layout_marginBottom="5dp" />

    <ImageView
        android:layout_width="30dp"
        android:layout_height="40dp"
        android:id="@+id/imageViewTransport"
        android:layout_below="@+id/textViewEventListHotelResDetails"
        android:layout_alignLeft="@+id/textViewVTAEventListTitle"
        android:layout_alignStart="@+id/textViewVTAEventListTitle"
        android:layout_marginLeft="20dp"/>


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Transport Res"
        android:id="@+id/textViewEventListTransportResDetails"
        android:layout_below="@+id/textViewEventListHotelResDetails"

        android:layout_alignBaseline="@+id/imageViewTransport"
        android:layout_toRightOf="@+id/imageViewTransport"
        android:layout_toEndOf="@+id/imageViewTransport"
        android:gravity="bottom"
        android:layout_marginLeft="10dp"
        android:textColor="#181da0"
        android:layout_marginBottom="5dp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:id="@+id/textViewSpace"
        android:layout_below="@+id/textViewEventListTransportResDetails"
        android:background="#0b021c" />



</RelativeLayout>