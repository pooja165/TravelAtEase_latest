<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:background="#d0d3d9">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewVTAEventListTitle"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/bright_foreground_inverse_material_dark"
        android:textSize="18sp"
        android:gravity="center_vertical|center_horizontal"
        android:textStyle="bold" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Place: "
        android:id="@+id/textView6"
        android:layout_below="@+id/textViewVTAEventListTitle"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/bright_foreground_inverse_material_dark"
        android:textSize="15sp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewVTAEventListPlace"
        android:layout_below="@+id/textViewVTAEventListTitle"
        android:layout_toRightOf="@+id/textView6"
        android:layout_toEndOf="@+id/textView6"
        android:textColor="@color/bright_foreground_inverse_material_dark"
        android:textSize="15sp" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="FromDate"
        android:id="@+id/textView2"
        android:layout_below="@+id/textViewVTAEventListPlace"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/bright_foreground_inverse_material_dark"
        android:textSize="15sp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Text"
        android:id="@+id/textViewVTAEventListFromDate"
        android:layout_below="@+id/textViewVTAEventListPlace"
        android:layout_toRightOf="@+id/textView2"
        android:layout_toEndOf="@+id/textView2"
        android:textColor="@color/bright_foreground_inverse_material_dark"
        android:textSize="15sp" />

    <TextView
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="To date"
        android:id="@+id/textView7"
        android:layout_below="@+id/textViewVTAEventListFromDate"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textColor="@color/bright_foreground_inverse_material_dark"
        android:textSize="15sp" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewVTAEventListToDate"
        android:layout_below="@+id/textViewVTAEventListFromDate"
        android:layout_toRightOf="@+id/textView7"
        android:layout_toEndOf="@+id/textView7"
        android:textColor="@color/bright_foreground_inverse_material_dark"
        android:textSize="15sp" />

</RelativeLayout>