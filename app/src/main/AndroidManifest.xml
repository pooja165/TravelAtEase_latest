<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="lshankarrao.travelatease1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ActionBarActivity"></activity>
        <activity android:name=".AddEditEventActivity"></activity>
        <activity android:name=".AddEditTripActivity"></activity>
        <activity android:name=".HotelReservationActivity"></activity>
        <activity android:name=".TransportReservationActivity"></activity>
        <activity android:name=".OtherReservationActivity"></activity>

    </application>

</manifest>
